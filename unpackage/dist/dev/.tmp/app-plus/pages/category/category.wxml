<view class="body"><wuc-tab vue-id="1" tab-list="{{tabList}}" tabCur="{{TabCur}}" tab-class="text-center" selectClass="text-black" data-event-opts="{{[['^updateTabCur',[['__set_sync',['$0','TabCur','$event'],['']]]],['^updateTabCur',[['__set_sync',['$0','TabCur','$event'],['']]]]]}}" bind:updateTabCur="__e" bind:__l="__l"></wuc-tab><swiper class="swiper" current="{{TabCur}}" duration="300" circular="{{true}}" indicator-color="rgba(255,255,255,0)" indicator-active-color="rgba(255,255,255,0)" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><swiper-item><view class="content"><block wx:for="{{icons.out}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="line"><block wx:for="{{item}}" wx:for-item="n" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['selectType',['$0'],[[['icons.out','',index],['','',i,'name']]]]]]]}}" class="item" bindtap="__e"><text class="{{['img',n.name==select?'active':'']}}"><view class="{{['_i','iconfont',n.icon]}}"></view></text><text class="text">{{n.name}}</text></view></block></view></block></view></swiper-item><swiper-item><view class="content"><block wx:for="{{icons.in}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="line"><block wx:for="{{item}}" wx:for-item="n" wx:for-index="i" wx:key="i"><view class="item"><text class="img"><view class="{{['_i','iconfont',n.icon]}}"></view></text><text class="text">{{n.name}}</text></view></block></view></block></view></swiper-item></swiper><uni-popup vue-id="2" show="{{showPopup}}" position="bottom" data-event-opts="{{[['^hidePopup',[['hidePopup']]]]}}" bind:hidePopup="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="insert_form"><view class="top"><view class="left"><text class="iconfont iconbeizhu"></text>备注:</view><input class="input" type="text" value="" placeholder="点击写备注..."/><text class="num">0.00</text></view><view class="grid"><view class="item">7</view><view class="item">8</view><view class="item">9</view><view class="item"><block wx:if="{{isToday}}"><text class="iconfont iconrili"></text>今天</block><block wx:else>2019/06/01</block></view><view class="item">4</view><view class="item">5</view><view class="item">6</view><view class="item">+</view><view class="item">1</view><view class="item">2</view><view class="item">3</view><view class="item">-</view><view class="item">.</view><view class="item">0</view><view class="item"><text class="iconfont iconiconfonttuige"></text></view><view class="item bg_main"><block wx:if="{{calc}}">=</block><block wx:else>完成</block></view></view></view></uni-popup></view>